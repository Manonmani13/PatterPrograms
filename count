class Solution {
    public int countSeniors(String[] details) {
        int count=0;
        for(String deta:details)
        {
            String ageStr=deta.substring(11,13);
            int age=Integer.parseInt(ageStr);
            if(age>60)
            {
                count++;
            }
        }
        return count;
        
    }
}
